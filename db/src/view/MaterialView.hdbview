VIEW "IV.MaterialView"(
	"MaterialId",
	"Material",
	"MaterialGroupId",
	"MaterialSeasonCategory") AS
		SELECT 
			"Material_$0"."MaterialId" AS "MaterialId",
			"Material_$0"."Material" AS "Material",
			"Material_$0"."MaterialGroupId" AS "MaterialGroupId",
			"Material_$0"."MaterialSeasonCategory" AS "MaterialSeasonCategory"
		FROM "Material" AS "Material_$0"
		WHERE ( "Material_$0"."SAPClient" = '100' );
